{
  "name": "maint_log_frontend_react",
  "author": "usarneme",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.19.2",
    "dompurify": "^2.0.8",
    "dotenv": "^8.2.0",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-axios": "^2.0.3",
    "react-dom": "^16.13.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "react-svg": "^11.0.18",
    "react-transition-group": "^4.3.0"
  },
  "scripts": {
    "start": "BROWSER=none REACT_APP_API_DOMAIN=http://localhost:7777 react-scripts start",
    "build": "REACT_APP_API_DOMAIN=http://maint-log.herokuapp.com react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "cov": "npm run test -- --coverage --watchAll=false --env=jsdom --coverageDirectory=src/__tests__/coverage/",
    "todos": "grep -r -n --color --include=*.js --exclude-dir=__tests__ TODO src/",
    "todos_files": "grep -r -l --color --include=*.js --exclude-dir=__tests__ TODO src/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-plugin-security": "^1.4.0",
    "react-test-renderer": "^16.13.1"
  },
  "jest": {
    "coverageReporters": [
      "text",
      "html"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/build/",
      "!<rootDir>/public/",
      "!<rootDir>/src/__tests__/coverage/"
    ]
  }
}
